{
    "editor.insertSpaces": true,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.rulers": [
        80
    ],
    "[asm]": {
        "editor.tabSize": 8
    },
    "[wla-dx-asm]": {
        "editor.tabSize": 8
    },
    "[markdown]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.wordWrap": "wordWrapColumn"
    },
    "ez80-asm.diagnosticProvider": false,
    "[ez80-asm]": {
        "editor.tabSize": 8,
        "editor.insertSpaces": false
    },
    "[log]": {
        "editor.wordWrap":"bounded"
    },
    "files.exclude": {
        "**/__*" : true
    },
    "files.associations": {
        "*.asm"  : "ez80-asm"
    },
    "files.readonlyInclude": {
        "cpm/bin/wla-dx/*" : true,
        "cpm/bin/RunCPM/*.exe" : true
    },
    "cSpell.words": [
        "concatenative",
        "pling"
    ],
    "Lua.runtime.version": "Lua 5.3"
}